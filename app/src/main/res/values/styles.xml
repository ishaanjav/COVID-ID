<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="welcomeET">
        <item name="android:textColor">#000</item>
        <item name="android:textColorHint">#8675C3</item>
        <item name="android:fontFamily">@font/remedium</item>
        <item name="background">@drawable/welcome_et</item>
    </style>

    <style name="BottomTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#424242</item>
        <item name="colorPrimaryDark">#202020</item>
    </style>

    <style name="WhiteTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#ffffff</item>
        <item name="colorPrimaryDark">#DCBEFF</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="SlidingDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/fast_fade_in</item>
        <item name="android:windowExitAnimation">@anim/fast_fade_out</item>
    </style>
</resources>